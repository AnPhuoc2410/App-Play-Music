<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryBackgroundColor">#FF1E1E1E</Color>
    <Color x:Key="AccentColor">#FF3BA55D</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource PrimaryBackgroundColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="White" />
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="#B3FFFFFF" />
</ResourceDictionary>